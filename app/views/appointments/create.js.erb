<% if @errors%>
    <% @errors.full_messages.each do |error|%>
        M.toast({html: '<%= j error %>', classes: 'rounded'})
    <% end %>
<% else %>
    $('form').trigger('reset');
    $('#results').empty()
    $('.modal-content').append('<%=j render "send_email"%>') 
    $('.box').html('<i class="material-icons camera_style">face</i>') 
    $('#modal').modal('open');
    setTimeout(()=> {
        $('#modal').modal('close');
        $('.modal-content').empty();
        window.location.href = "/"
    }, 5000)
<% end %>
